<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_books">

    <fragment
        android:id="@+id/navigation_books"
        android:name="com.example.theoneapp.ui.books.bookList.BooksFragment"
        android:label="Books"
        tools:layout="@layout/fragment_books" />

    <fragment
        android:id="@+id/navigation_characters"
        android:name="com.example.theoneapp.ui.characters.characterList.CharactersFragment"
        android:label="Characters"
        tools:layout="@layout/fragment_characters" >
    </fragment>

    <fragment
        android:id="@+id/navigation_movies"
        android:name="com.example.theoneapp.ui.movies.movieList.MoviesFragment"
        android:label="Movies"
        tools:layout="@layout/fragment_movies" />
</navigation>